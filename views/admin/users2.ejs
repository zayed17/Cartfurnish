<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">

  <!-- Boxicons -->
  <link href='https://unpkg.com/boxicons@2.0.9/css/boxicons.min.css' rel='stylesheet'>

  <!-- My CSS -->
  <link rel="stylesheet" href="/adminhome/style.css">
  
  <link rel="stylesheet" href="https://cdn.datatables.net/1.13.2/css/jquery.dataTables.min.css">

  <title>Users | NFT Arena</title>
</head>

<body>


  <!-- SIDEBAR -->
  <section id="sidebar">
    <a href="dashboard" class="brand">
      <i class='bx bxs-smile'></i>
      <span class="text">NFT Arena</span>
    </a>
    <ul class="side-menu top">
      <li>
        <a href="dashboard">
          <i class='bx bxs-dashboard'></i>
          <span class="text">Dashboard</span>
        </a>
      </li>
      <li>
        <a href="products">
          <i class='bx bxl-product-hunt'></i>
          <span class="text">Products</span>
        </a>
      </li>
      <li>
        <a href="category">
          <i class='bx bxs-category-alt'></i>
          <span class="text">Categories</span>
        </a>
      </li>
      <li>
        <a href="author">
          <i class='bx bxs-message-dots'></i>
          <span class="text">Authors</span>
        </a>
      </li>
      <li class="active">
        <a href="user">
          <i class='bx bxs-group'></i>
          <span class="text">Users</span>
        </a>
      </li>
      <li>
        <a href="banners">
          <i class='bx bxs-carousel'></i>
          <span class="text">Banners</span>
        </a>
      </li>
      <li>
        <a href="orders">
          <i class='bx bxs-shopping-bag-alt'></i>
          <span class="text">Orders</span>
        </a>
      </li>
      <li>
        <a href="/admin/coupons">
          <i class='bx bxs-coupon'></i>
          <span class="text" >Coupons</span>
        </a>
      </li>

    </ul>
    <ul class="side-menu">
      <li>
        <a href="">
          <i class='bx bxs-cog'></i>
          <span class="text" style="text-decoration: line-through;">Settings</span>
        </a>
      </li>
      <li>
        <a href="/admin/adminlogout" class="logout">
          <i class='bx bxs-log-out-circle'></i>
          <span class="text">Logout</span>
        </a>
      </li>
    </ul>
  </section>
  <!-- SIDEBAR -->



  <!-- CONTENT -->
  <section id="content">
    <!-- NAVBAR -->
    <nav>
      <i class='bx bx-menu'></i>
      <a href="#" class="nav-link">Search whole data</a>
      <form action="#">
        <div class="form-input">
          <input type="search" placeholder="Search...">
          <button type="submit" class="search-btn"><i class='bx bx-search'></i></button>
        </div>
      </form>
      <input type="checkbox" id="switch-mode" hidden>
      <label for="switch-mode" class="switch-mode"></label>
      <a href="#" class="notification">
        <i class='bx bxs-bell'></i>
        <span class="num">8</span>
      </a>
      <a href="#" class="profile">
        <img
          src="data:image/png;base64,">
      </a>
    </nav>
    <!-- NAVBAR -->

    <!-- MAIN -->
    <main>
      <div class="head-title">
        <div class="left">
          <h1>Users</h1>
          <ul class="breadcrumb">
            <li>
              <a href="#">Dashboard</a>
            </li>
            <li><i class='bx bx-chevron-right'></i></li>
            <li>
              <a class="active" href="#">Users</a>
            </li>
          </ul>
        </div>
      </div>


      <div class="table-data">
        <div class="order">


          <% if(userData.length>0){ %>

            <table id="tablePagination" class="display">
              <thead>
                <tr>
                  <th>
                    <center> Sl.No.</center>
                  </th>
                  <th>
                    <center>Name</center>
                  </th>
                  <th>
                    <center>Email</center>
                  </th>
                  <th>
                    <center>Mobile</center>
                  </th>
                  <th>
                    <center>Block / Unblock</center>
                  </th>
                </tr>
              </thead>
              <tbody>

                <% for(let i=0;i<userData.length;i++){ %>
                  <tr>
                    <td align="center">
                      <%=i+1%>
                    </td>
                    <td align="center">
                      <p>
                        <%= userData[i].name %>
                      </p>
                    </td>
                    <td align="center">
                      <p>
                        <%= userData[i].email %>
                      </p>
                    </td>
                    <td align="center">
                      <%= userData[i].mobile %>
                    </td>
                    <td align="center">
                      <p>
                        <% if(userData[i].isBlocked==false){%>
                            <span class="status block-button" style="background-color: green;"
                            data-id="<%= userData[i]._id %>">
                            Active</span>
                          </a>

                          <%}else{%>
                              <span class="status unBlock-button" style="background: red;"
                              data-id="<%= userData[i]._id %>">
                              Blocked</span>
                            </a>
                            <%}%>
                      </p>
                    </td>


                  </tr>
                  <% } %>




              </tbody>
            </table>

            <% } else { %>
              No Users found in database.
              <% } %>




    </main>
    <!-- MAIN -->
  </section>


<!--  SweetAlert2 script -->
   <script src="https://cdn.jsdelivr.net/npm/sweetalert2@10"></script>
   <!-- block -->
  <script>
    const blockButtons = document.querySelectorAll('.block-button');
    blockButtons.forEach((blockButton) => {
      blockButton.addEventListener('click', (event) => {
        event.preventDefault();
        const authorId = blockButton.getAttribute('data-id');
        Swal.fire({
          title: 'Are you sure to block User ?',
          icon: 'warning',
          showCancelButton: true,
          confirmButtonColor: '#d33',
          cancelButtonColor: '#3085d6',
          confirmButtonText: 'Yes, Block'
        }).then((result) => {
          if (result.isConfirmed) {
            window.location.href = `/admin/blockUser?id=${authorId}`;
          }
        });
      });
    });
  </script>

  <!-- unBlock-button -->
  <script>
    const unBlockButtons = document.querySelectorAll('.unBlock-button');
    unBlockButtons.forEach((unBlockButton) => {
      unBlockButton.addEventListener('click', (event) => {
        event.preventDefault();
        const authorId = unBlockButton.getAttribute('data-id');
        Swal.fire({
          title: 'Are you sure to un-block User ?',
          icon: 'warning',
          showCancelButton: true,
          confirmButtonColor: '#d33',
          cancelButtonColor: '#3085d6',
          confirmButtonText: 'Yes, un-block'
        }).then((result) => {
          if (result.isConfirmed) {
            window.location.href = `/admin/blockUser?id=${authorId}`;
          }
        });
      });
    });
  </script>

  <script src="/adminhome/script.js"></script>
  <script src="https://code.jquery.com/jquery-3.5.1.js"></script>
  <script src="https://cdn.datatables.net/1.13.2/js/jquery.dataTables.min.js"></script>
  <script type="text/javascript" charset="utf8"
    src="https://cdn.datatables.net/1.13.2/js/jquery.dataTables.js"></script>
  <script>
    $(document).ready(function () {
      $('#tablePagination').DataTable();
    });
  </script>
</body>

</html>