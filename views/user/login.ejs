							<div class="tab-content">
                                <% if (typeof success !=='undefined' ) { %>
                                    <p id="success" style="color: rgb(6, 211, 20);">
                                        <%= success %>
                                    </p>
                                    <script>
                                        // Set a timer to remove the message after 5000 milliseconds (5 seconds)
                                        setTimeout(function () {
                                            var messageElement = document.getElementById('success');
                                            if (messageElement) {
                                                messageElement.style.display = 'none';
                                            }
                                        }, 3500);
                                    </script>
                                    <% } %> 
							    
							    <div class="tab-pane fade show active" id="register-2" role="tabpanel" aria-labelledby="register-tab-2">
                                    <% if (typeof message !=='undefined' ) { %>
                                        <p id="message" style="color: rgb(255, 0, 0);">
                                            <%= message %>
                                        </p>
                                        <script>
                                            // Set a timer to remove the message after 5000 milliseconds (5 seconds)
                                            setTimeout(function () {
                                                var messageElement = document.getElementById('message');
                                                if (messageElement) {
                                                    messageElement.style.display = 'none';
                                                }
                                            }, 3500);
                                        </script>
                                        <% } %> 
                                        <style>
                                            .is-invalid {
                                                border: 1px solid #f44336; /* Red border */
                                                border-radius: 0.2rem; /* Small border-radius */
                                                transition: border-color 0.3s ease-out; /* Smoother transition */
                                            }
                                        
                                            .invalid-feedback {
                                                display: none;
                                                font-size: 80%; /* Smaller font size */
                                                color: #f44336; /* Red color for error messages */
                                                transition: opacity 0.3s ease-out; /* Smoother transition */
                                            }
                                        </style>
                                        
                                        <form action="/login" method="post" >
                                            <div class="form-group">
                                                <label for="email">Email *</label>
                                                <input type="email" class="form-control" id="register-email-2" name="email" placeholder="Enter email" required>
                                                <div class="invalid-feedback" id="email-error-message">
                                                    Please enter a valid email address.
                                                </div>
                                            </div>
                                        
                                            <div class="form-group">
                                                <label for="password">Password *</label>
                                                <input type="password" class="form-control" id="register-password-2" placeholder="Enter password" name="password" required>
                                                <div class="invalid-feedback" id="password-error-message">
                                                    Please enter a password.
                                                </div>
                                            </div>
                                        
                                            <div class="form-footer">
                                                <button type="button" class="btn btn-outline-primary-2" onclick="validateFormAndSubmit()">
                                                    <span>LOGIN IN </span>
                                                    <i class="icon-long-arrow-right"></i>
                                                </button>
                                        
                                                <a href="/forgot" class="btn btn-link" id="forgot-password-link">Forgot Password?</a>
                                        
                                                <div>
                                                    <p>New User <a href="/signup">click here!</a></p>
                                                    <p><a href="/loginwithotp">Login with OTP</a></p>
                                                </div>
                                            </div>
                                        </form>
                                        

                                        <script>
                                            function validateFormAndSubmit() {
                                                var emailInput = document.getElementById('register-email-2');
                                                var passwordInput = document.getElementById('register-password-2');
                                                var emailErrorMessage = document.getElementById('email-error-message');
                                                var passwordErrorMessage = document.getElementById('password-error-message');
                                                var isValid = true;
                                        
                                                emailInput.classList.remove('is-invalid');
                                                passwordInput.classList.remove('is-invalid');
                                        
                                                document.querySelectorAll('.invalid-feedback').forEach(function (element) {
                                                    element.style.display = 'none';
                                                });
                                        
                                                if (!emailInput.value.trim() || !isValidEmail(emailInput.value.trim())) {
                                                    emailInput.classList.add('is-invalid');
                                                    emailErrorMessage.style.display = 'block';
                                                    isValid = false;
                                                }
                                        
                                                if (!passwordInput.value.trim()) {
                                                    passwordInput.classList.add('is-invalid');
                                                    passwordErrorMessage.style.display = 'block';
                                                    isValid = false;
                                                }
                                        
                                                if (isValid) {
                                                    document.forms[0].submit(); 
                                                }
                                        
                                                setTimeout(function () {
                                                    document.querySelectorAll('.invalid-feedback').forEach(function (element) {
                                                        element.style.display = 'none';
                                                    });
                                                }, 3000);
                                            }
                                        
                                            function isValidEmail(email) {
                                                var emailRegex = /^[^\s@]+@[^\s@]+\.[^\s@]+$/;
                                                return emailRegex.test(email);
                                            }
                                        </script>
                                        
                                        
							    	
